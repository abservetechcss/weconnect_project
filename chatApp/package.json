{
  "name": "custom-webchat",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --env target=widget --mode=production",
    "start": "webpack serve --env target=widget --mode=development",
    "test": "jest",
    "train:ner": "ts-node src/nlp/tasks/ner/train.ts",
    "train:intent-classification": "ts-node src/nlp/tasks/intent-classification/train.ts"
  },
  "jest": {
    "rootDir": "tests",
    "transformIgnore  Patterns": [
      "/node_modules/(?!@botonic).+\\.(js|jsx|ts|tsx|mjs)$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@botonic/react": "^0.20.0",
    "@emotion/core": "^10.3.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@emotion/weak-memoize": "^0.2.5",
    "@mui/material": "^5.6.0",
    "@mui/styled-engine-sc": "^5.10.14",
    "classnames": "^2.3.2",
    "moment-timezone": "^0.5.34",
    "qs": "^6.10.5",
    "react-calendar": "2.19.2",
    "react-icons": "^4.4.0",
    "react-infinite-scroller": "^1.2.6",
    "react-scrollbars-custom": "^4.1.1",
    "react-use-storage": "^0.5.1",
    "styled-components": "^5.3.6",
    "svg-parser": "^2.0.4",
    "webfontloader": "^1.6.28",
    "whatwg-fetch": "^3.6.2",
    "yet-another-abortcontroller-polyfill": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@hot-loader/react-dom": "^17.0.1",
    "@svgr/webpack": "^6.3.1",
    "analytics-node": "^3.4.0-beta.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^2.0.7",
    "chokidar": "^3.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
