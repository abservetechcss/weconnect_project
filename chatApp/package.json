    "imagemin-gifsicle": "^6.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^5.0.0",
    "isomorphic-style-loader": "^5.3.2",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^5.0.0",
